{
  "generator-angular-fullstack": {
    "endpointDirectory": "server/api/",
    "insertRoutes": true,
    "registerRoutesFile": "server/routes.js",
    "routesNeedle": "// Insert routes below",
    "routesBase": "/api/",
    "pluralizeRoutes": true,
    "insertSockets": true,
    "registerSocketsFile": "server/config/websockets.js",
    "socketsNeedle": "// Insert sockets below",
    "insertModels": true,
    "registerModelsFile": "server/sqldb/index.js",
    "modelsNeedle": "// Insert models below",
    "filters": {
      "js": true,
      "ts": true,
      "flow": false,
      "html": true,
      "sass": true,
      "ngroute": true,
      "bootstrap": true,
      "uibootstrap": true,
      "ws": true,
      "auth": true,
      "models": true,
      "mongooseModels": true,
      "mongoose": true,
      "mocha": true,
      "jasmine": false,
      "expect": true
    }
  },
  "generator-ng-component": {
